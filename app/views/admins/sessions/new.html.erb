<div class="container"> 
      <h2 class="mt-3">管理者ログイン</h2>
      <%= form_with model: @admin, url: admin_session_path,local:true do |f| %>
          <div class="form-group">
            <%= f.label :"メールアドレス"%>
            <%= f.email_field :email, autofocus: true, autocomplete: "email",:placeholder => 'sumple@example.com',class: "form-control"%>
          </div>
        
          <div class="form-group">
            <%= f.label :"パスワード"%>
            <%= f.password_field :password, autocomplete: "current-password",class: "form-control"%>
          </div>
            <%= f.submit "ログイン",class: "new-btn btn btn-primary" %>
      <% end %>
      
      <%= render "admins/shared/links" %>
    
</div>