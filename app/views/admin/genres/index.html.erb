<div>
  <h1>ジャンル一覧・追加</h1>
    <%=form_with model: @genre,url: admin_genres_path,method: :post,local:true do |f|%>
    <label>ジャンル名</label>
     <%=f.text_field:name%>
     <%=f.submit '新規登録'%>
    <%end%>
</div>

<div>
  <h1>ジャンル</h1>
  <% @genres.each do |genre|%>
    <%=genre.name%>
    <%=link_to '編集する',edit_admin_genre_path(genre.id)%><br>
  <%end%>
</div>