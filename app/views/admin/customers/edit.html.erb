    <h2><%=@customer.family_name%><%=@customer.first_name%>さんの会員情報編集</h2>
      <%=form_with model: @customer,url: admin_customer_path,method: :patch,local:true do |f|%>
        <div>
          <%=f.label:"会員ID"%>
          <%=f.label:"#{@customer.id}"%>
        </div>
        <div>
          <%=f.label:"氏名"%>
          <%=f.text_field:family_name%><%=f.text_field:first_name%>
        </div>
        <div>
          <%=f.label:"フリガナ"%>
          <%=f.text_field:family_name_kana%><%=f.text_field:first_name_kana%>
        </div>
        <div>
          <%=f.label:"郵便番号"%>
          <%=f.text_field:postcode%>
        </div>
        <div>
          <%=f.label:"住所"%>
          <%=f.text_field:address%>
        </div>
        <div>
          <%=f.label:"電話番号"%>
          <%=f.text_field:phonenumber%>
        </div>
        <div>
          <%=f.label:"メールアドレス"%>
          <%=f.email_field:email%>
        </div>
        <div>
          <%=f.label:"会員ステータス"%>
          <%=f.radio_button:is_deleted,false%>
          <%=f.label:"有効"%>
          <%=f.radio_button:is_deleted,true%>
          <%=f.label:"退会"%>
        </div>
        <div>
          <%=f.submit "変更を保存"%>
        </div>
      <%end%>